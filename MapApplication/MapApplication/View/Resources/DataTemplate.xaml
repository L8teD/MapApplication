<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:v="clr-namespace:MapApplication.View"
                    xmlns:vm="clr-namespace:MapApplication.ViewModel">
    <DataTemplate DataType="{x:Type vm:PlotVM}">
        <v:Plot></v:Plot>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:PlotControlVM}">
        <v:PlotControl></v:PlotControl>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:PlotTableVM}">
        <v:PlotTable></v:PlotTable>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:LegendButtonVM}">
        <v:LegendElement></v:LegendElement>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:LegendVM}">
        <v:Legend></v:Legend>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:DataRowVM}">
        <v:DataRow></v:DataRow>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:DataTableVM}">
        <v:DataTable></v:DataTable>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:DataRowWithChangesVM}">
        <v:DataRowWithChange></v:DataRowWithChange>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:DataTableWithChangesVM}">
        <v:DataTableWithChanges></v:DataTableWithChanges>
    </DataTemplate>
</ResourceDictionary>