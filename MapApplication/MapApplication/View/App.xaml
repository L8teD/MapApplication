<Application x:Class="MapApplication.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:MapApplication"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Resources/DataTemplate.xaml"></ResourceDictionary>
                <ResourceDictionary Source="Styles/ButtonDict.xaml"></ResourceDictionary>
                <ResourceDictionary Source="Styles/BorderDict.xaml"></ResourceDictionary>
                <ResourceDictionary Source="Styles/LabelDict.xaml"></ResourceDictionary>
                <ResourceDictionary Source="Styles/ListViewDict.xaml"></ResourceDictionary>
                <ResourceDictionary Source="Styles/ComboBoxDict.xaml"></ResourceDictionary>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
